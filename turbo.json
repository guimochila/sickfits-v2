{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["@sickfits/utils#build"],
      "cache": false,
      "outputs": [
        "build/**"
      ]
    }
  }
}
